{
  "name": "pandacss-theme-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "biome:fix": "biome check --max-diagnostics 50 --apply .",
    "biome:lint": "biome lint . --max-diagnostics 50",
    "build": "next build",
    "dev": "next dev",
    "env:init": "if [ ! -f .env ]; then cp .env.example .env; fi",
    "format": "pnpm biome:fix && pnpm prettier:fix && sort-package-json",
    "postinstall": "sort-package-json",
    "lint": "pnpm typecheck && pnpm biome:lint && pnpm prettier:lint",
    "lint:fix": "pnpm format",
    "prepare": "panda codegen",
    "prettier:fix": "prettier --ignore-path '.prettierignore' --write \"**/*.{json,yml,yaml,css,scss,md,mdx}\"",
    "prettier:lint": "prettier --ignore-path '.prettierignore' --check \"**/*.{json,yml,yaml,css,scss,md,mdx}\"",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/anatomy": "^3.2.0",
    "@ark-ui/react": "^3.0.2",
    "@pandacss/config": "^0.40.0",
    "@pandacss/generator": "^0.40.0",
    "@pandacss/logger": "^0.40.0",
    "@pandacss/parser": "^0.40.0",
    "@pandacss/preset-base": "^0.40.0",
    "@pandacss/preset-panda": "^0.40.0",
    "@vercel/analytics": "^1.3.1",
    "clsx": "^2.1.1",
    "colorjs.io": "^0.5.0",
    "geist": "^1.3.0",
    "lucide-react": "^0.381.0",
    "next": "^14.2.3",
    "next-themes": "^0.3.0",
    "outdent": "^0.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@pandacss/dev": "^0.40.0",
    "@pandacss/types": "^0.40.0",
    "@types/node": "^20.12.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.10.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
