{
  "name": "pandacss-theme-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "biome:fix": "biome check --max-diagnostics 50 --apply .",
    "biome:lint": "biome lint . --max-diagnostics 50",
    "build": "next build",
    "build:prod": "turbo run build",
    "dev": "next dev",
    "env:init": "if [ ! -f .env ]; then cp .env.example .env; fi",
    "format": "pnpm biome:fix && pnpm prettier:fix && sort-package-json",
    "postinstall": "sort-package-json",
    "lint": "pnpm typecheck && pnpm biome:lint && pnpm prettier:lint",
    "lint:fix": "pnpm format",
    "prepare": "panda codegen",
    "prettier:fix": "prettier --ignore-path '.prettierignore' --write \"**/*.{json,yml,yaml,css,scss,md,mdx}\"",
    "prettier:lint": "prettier --ignore-path '.prettierignore' --check \"**/*.{json,yml,yaml,css,scss,md,mdx}\"",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/anatomy": "^3.2.0",
    "@ark-ui/react": "^3.0.1",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/mdx": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.3",
    "@types/mdx": "^2.0.13",
    "@vercel/analytics": "^1.3.1",
    "animate.css": "^4.1.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.2.6",
    "geist": "^1.3.0",
    "lucide-react": "^0.379.0",
    "next": "^14.2.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-schemaorg": "^2.0.0",
    "schema-dts": "^1.1.2",
    "server-only": "^0.0.1",
    "sharp": "0.33.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@pandacss/dev": "^0.39.2",
    "@pandacss/preset-panda": "^0.39.2",
    "@pandacss/types": "^0.39.2",
    "@radix-ui/colors": "^3.0.0",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "gray-matter": "^4.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "pwa-asset-generator": "^6.3.1",
    "sort-package-json": "^2.10.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
