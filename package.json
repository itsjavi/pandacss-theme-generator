{
  "name": "pandacss-theme-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "biome:fix": "biome check --max-diagnostics 50 --write .",
    "biome:lint": "biome lint . --max-diagnostics 50",
    "build": "next build",
    "dev": "pnpm open && next dev",
    "env:init": "if [ ! -f .env ]; then cp .env.example .env; fi",
    "format": "pnpm biome:fix && pnpm prettier:fix && sort-package-json",
    "postinstall": "sort-package-json",
    "lint": "pnpm typecheck && pnpm biome:lint && pnpm prettier:lint",
    "lint:fix": "pnpm format",
    "open": "sleep 3 && open http://localhost:3000 &",
    "prepare": "panda codegen",
    "prettier:fix": "prettier --ignore-path '.prettierignore' --write \"**/*.{json,yml,yaml,css,scss,md,mdx}\"",
    "prettier:lint": "prettier --ignore-path '.prettierignore' --check \"**/*.{json,yml,yaml,css,scss,md,mdx}\"",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/anatomy": "^3.4.0",
    "@ark-ui/react": "^3.5.0",
    "@pandacss/preset-base": "^0.41.0",
    "@pandacss/preset-panda": "^0.41.0",
    "@vercel/analytics": "^1.3.1",
    "clsx": "^2.1.1",
    "culori": "^4.0.1",
    "geist": "^1.3.0",
    "immer": "^10.1.1",
    "jotai": "^2.8.4",
    "jotai-immer": "^0.4.1",
    "lucide-react": "^0.400.0",
    "nanoid": "^5.0.7",
    "next": "^14.2.4",
    "next-themes": "^0.3.0",
    "outdent": "^0.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@pandacss/dev": "^0.41.0",
    "@pandacss/types": "^0.41.0",
    "@types/culori": "^2.1.1",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "prettier": "^3.3.2",
    "sort-package-json": "^2.10.0",
    "typescript": "^5.5.3"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
